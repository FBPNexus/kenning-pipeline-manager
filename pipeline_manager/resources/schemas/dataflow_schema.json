{
    "$id": "dataflow_schema",
    "description": "Schema used to validate dataflows",
    "type": "object",
    "properties": {
        "version": {
            "description": "Version of the dataflow",
            "type": "string"
        },
        "metadata": {
            "$ref": "metadata_schema"
        },
        "subgraphs": {
            "type": "array",
            "items": {
                "$ref": "graph_schema"
            }
        },
        "graph": {
            "$ref": "graph_schema"
        }
    },
    "required": [
        "graph"
    ],
    "unevaluatedProperties": false
}
